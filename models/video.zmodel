import "./user.zmodel"

model Video {
    id                String        @id @default(uuid())  
    url              String          
    title          String?          
    description  String?          
    datePosted  String?          
    userId        String?          
    user            User?            @relation(fields: [userId], references: [id], name:"user")  
    hasAudio       Boolean        @default(false)

    dateCreated DateTime @default(now())
    dateUpdated DateTime @updatedAt  @default(now())
    @@allow("all", true)
}
